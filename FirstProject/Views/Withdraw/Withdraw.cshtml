@{
    Layout = null;
    ViewBag.Title = "Withdraw Page";
}

<head>
    <link rel="stylesheet" href="@Url.Content("~/css/withdraw.css")" />
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="~/js/Withdraw.js"></script>
</head>
<div class="withdraw-container">
    <h2>Withdraw Money</h2>
    <form action="" method="post">
        <label for="amount">Amount:</label>
        <input type="text" id="withdrawAmount" name="amount" required>
        <button type="button" id="withdrawButton">Withdraw</button>
    </form>
    <a href="/Home" class="back-button">Back</a>
</div>

